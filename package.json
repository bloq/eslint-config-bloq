{
  "name": "eslint-config-bloq",
  "version": "1.2.1",
  "description": "ESLint config to use at Bloq",
  "keywords": [
    "bloq",
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Gabriel Montes <gabriel@bloq.com>",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "repository": "bloq/eslint-config-bloq",
  "scripts": {
    "lint": "eslint --cache .",
    "precommit": "npm run lint",
    "check": "npm-check -i eslint-config* -i husky",
    "prepublish": "npm run lint && npm run check"
  },
  "devDependencies": {
    "eslint": "4.19.0",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsdoc": "3.5.0",
    "eslint-plugin-mocha": "4.12.1",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prefer-arrow": "1.1.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-standard": "3.0.1",
    "husky": "0.14.3",
    "npm-check": "5.5.2"
  },
  "peerDependencies": {
    "eslint": ">= 3",
    "eslint-config-standard": ">= 11",
    "eslint-plugin-import": ">= 2",
    "eslint-plugin-jsdoc": ">= 3",
    "eslint-plugin-mocha": ">= 4",
    "eslint-plugin-node": ">= 6",
    "eslint-plugin-prefer-arrow": ">= 1",
    "eslint-plugin-promise": ">= 3",
    "eslint-plugin-standard": ">= 3"
  },
  "preferGlobal": false,
  "private": false
}
